<%= simple_form_for([@centro,@coordinador], :html => { :class => 'form-horizontal'}) do |f| %>
  <%= f.input :nombre %>
  <%= f.input :email %>
  <div class="form-actions">
    <%= f.button :submit, :class => 'btn-primary' %>
    <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
                centro_coordinadores_path(@centro), :class => 'btn' %>
  </div>
<% end %>
