<%- model_class = Asignacioncomite -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong>Nombre</strong></dt>
  	<dd><%= @asignacioncomite.nombres %></dd>

  <dt><strong>Apellido</strong></dt>
  	<dd><%= @asignacioncomite.apellidos %></dd>

  <dt><strong>Programa</strong></dt>
  	<dd><%= @asignacioncomite.programa.abreviatura %></dd>

  <dt><strong>Ficha</strong></dt>
  	<dd><%= @asignacioncomite.ficha %></dd>

  <dt><strong>Telefono</strong></dt>
  	<dd><%= @queja.telefono %></dd>

  <dt><strong>Numero de: <%= @queja.tipo_documento.sigla %> </strong></dt>
  	<dd><%= @queja.identificacion %></dd>
</dl>

